# Configurações de Retry
INTEGRATION_RETRY_MAX_ATTEMPTS=3
INTEGRATION_RETRY_INITIAL_DELAY=1000
INTEGRATION_RETRY_MAX_DELAY=10000
INTEGRATION_RETRY_BACKOFF_FACTOR=2

# Configurações de Circuit Breaker
INTEGRATION_CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
INTEGRATION_CIRCUIT_BREAKER_RESET_TIMEOUT=60000
INTEGRATION_CIRCUIT_BREAKER_HALF_OPEN_TIMEOUT=30000

# Configurações de Monitoramento
INTEGRATION_MONITORING_ENABLED=true
INTEGRATION_MONITORING_LOG_LEVEL=info

# Configurações de Banco de Dados
DATABASE_URL="postgresql://test:test@localhost:5432/test_db"

# Configurações de Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=test 